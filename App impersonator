CREATE TABLE fitness (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    name TEXT,
    workout_type TEXT,
    minutes INTEGER);

INSERT INTO fitness (name, workout_type, minutes) VALUES ("Demetri", "push_up", 5);
INSERT INTO fitness (name, workout_type, minutes) VALUES ("Ly", "squat", 2);
INSERT INTO fitness (name, workout_type, minutes) VALUES ("Yen", "running", 30);
INSERT INTO fitness (name, workout_type, minutes) VALUES ("Tam", "walking", 60);

UPDATE fitness SET name = "Yen"
WHERE id = 3;

DELETE FROM fitness 
WHERE id = 4;
